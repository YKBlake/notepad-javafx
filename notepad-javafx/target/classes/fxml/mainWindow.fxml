<AnchorPane fx:id="mainAnchorPane" maxHeight="720.0" maxWidth="428.0" minHeight="720.0" minWidth="428.0"
            prefHeight="720.0" prefWidth="428.0" xmlns="http://javafx.com/javafx/17"
            xmlns:fx="http://javafx.com/fxml/1">
