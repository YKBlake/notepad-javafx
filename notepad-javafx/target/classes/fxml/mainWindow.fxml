        </font>
    </Label>
    <Button fx:id="noteAddButton" layoutX="14.0" layoutY="12.0" maxHeight="50.0" maxWidth="50.0" minHeight="50.0"
            minWidth="50.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" text="+">
        <font>
          <Font size="24.0"/>
        </font>
    </Button>
    <Line endX="327.0" layoutX="100.0" layoutY="76.0" startX="-100.0"/>
    <ScrollPane fx:id="noteScrollPane" layoutY="77.0" maxHeight="644.0" maxWidth="428.0" minHeight="644.0"
                minWidth="428.0" prefHeight="644.0" prefWidth="428.0">
        <FlowPane fx:id="noteflowpane" hgap="10.0" layoutY="77.0" prefHeight="644.0" prefWidth="412.0" vgap="10.0">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </FlowPane>
    </ScrollPane>
</AnchorPane>
